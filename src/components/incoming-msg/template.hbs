<div class = "incoming-msg-container">
    <div class = "incoming-msg">
        {{#if msgImg}}
            <img src = "{{msgImg}}" class = "img" alt = "{{msgText}}"/>
        {{/if}}
        <div class = "text">{{msgText}}</div>
        <div class = "time">{{msgTime}}</div>
    </div>    
</div>    
